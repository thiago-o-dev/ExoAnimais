[node name="CoralSol" type="Node" parent="."]
script = ExtResource("1_qtkit")
image = ExtResource("13_ml3ar")
title = "Coral-sol"
subtitle = "Tubastraea coccinea"
description = "O coral-sol é originário da região do Indo-Pacífico e chegou ao Brasil por bioincrustação em cascos de navios e plataformas de petróleo. Foi registrado pela primeira vez no litoral do Rio de Janeiro e hoje se espalha por diversas regiões da costa brasileira. Ele cresce rapidamente sobre recifes e rochas, competindo com corais nativos e prejudicando a biodiversidade marinha ao impedir o desenvolvimento de outras espécies."
success_text = "Você retornou o animal exótico brasileiro ao seu habitat natural na África ocidental!"
